eturn '未知错误'\n  }\n}\n\nexport default function (config) {\n\n  commitMutation(\n    environment,\n    { \n      ...config,\n      variables: {\n        ...config.variables,\n        input: {\n          ...config.variables.input,\n          clientMutationId: new Date().getTime().toString()\n        }\n      },\n      onCompleted: (response, errors) => {\n        const errorMessage = errors ? pickNetworkError(errors) : null\n        if(config.onCompleted) {\n          config.onCompleted(response, errorMessage)\n        } else {\n          if(errorMessage) {\n            alert(errorMessage)\n          }\n        }\n      },\n      onError: (error) => {\n        if(config.onError) {\n          config.onError(error)\n        } else {\n          if(error) {\n            alert(`刚才网络出现了问题，请再试一次`)\n          }\n        }\n      }\n    }\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"f584c71a7f4da2659f5f9c3ddca0cdf8","cacheData":{"env":{}}}