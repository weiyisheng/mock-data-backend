 [deleteModal, setDeleteModal] = useState(false);\n  const [selectedProduct, setSelectedProduct] = useState(null);\n\n  function handleEdit(product) {\n    setSelectedProduct(product, () => {\n      setAddModalVisible(true);\n    });\n  }\n\n  function handleDelete(product) {\n    setSelectedProduct(product, () => {\n      setAddModalVisible(true);\n    });\n  }\n\n  const products = (get(viewer, \"products.edges\") || []).map(e => e.node);\n\n  return (\n    <div className=\"products-container\">\n      <Button onClick={() => setAddModalVisible(!addModalVisible)}>\n        添加项目\n      </Button>\n      <Table\n        columns={columns(handleEdit, handleDelete)}\n        dataSource={products}\n      />\n      <AddProductModal\n        viewerID={viewer.id}\n        visible={addModalVisible}\n        close={() => setAddModalVisible(false)}\n      />\n    </div>\n  );\n}\n\nexport default createFragmentContainer(ProductList, {\n  viewer: graphql`\n    fragment ProductList_viewer on Viewer {\n      id\n      products(first: 100)\n        @connection(key: \"ProductList_products\", filters: []) {\n        edges {\n          node {\n            id\n            name\n            mocks {\n              count\n            }\n          }\n        }\n      }\n    }\n  `\n});\n"},"lineCount":null}},"error":null,"hash":"cd2eaf7a32344ac601df7880259ae67d","cacheData":{"env":{}}}